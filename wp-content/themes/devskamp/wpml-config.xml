<wpml-config>
    <custom-types>
        <custom-type translate="1">wpcf7_contact_form</custom-type>
        <custom-type translate="1">tt-portfolio</custom-type>
        <custom-type translate="1">tt-joblist</custom-type>
        <custom-type translate="1">tt-member</custom-type>
        <custom-type translate="1">tt-service</custom-type>
        <custom-type translate="1">vc_grid_item</custom-type>
    </custom-types>
    <taxonomies>
       <taxonomy translate="1">tt-portfolio-cat</taxonomy>
       <taxonomy translate="1">tt-joblist-cat</taxonomy>
    </taxonomies>
    <custom-fields>
        <custom-field action="translate">materialize_qoute</custom-field>
        <custom-field action="translate">materialize_qoute_author</custom-field>
        <custom-field action="translate">materialize_link_text</custom-field>
        <custom-field action="translate">materialize_link_title</custom-field>
        <custom-field action="translate">materialize_portfolio_link_text</custom-field>
        <custom-field action="translate">materialize_portfolio_price</custom-field>
        <custom-field action="translate">materialize_team_designation</custom-field>
        <custom-field action="translate">materialize_barchart_details</custom-field>
        <custom-field action="translate">materialize_page_subtitle</custom-field>
        <custom-field action="copy">materialize_post_views_count</custom-field>
    </custom-fields>
    <admin-texts>
        <key name="materialize_theme_option">
            <key name="header-contact"/>
            <key name="blog-title"/>
            <key name="text-logo"/>
            <key name="404_text"/>
            <key name="404_subtext"/>
            <key name="404_desc"/>
            <key name="404_button_text"/>
            <key name="footer-intro-title"/>
            <key name="footer-text"/>
        </key>
    </admin-texts>
</wpml-config>